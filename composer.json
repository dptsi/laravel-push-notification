{
    "name": "dptsi/laravel-push-notification",
    "description": "Laravel Firebase Cloud Messaging.",
    "homepage": "https://github.com/dptsi/laravel-push-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Nusantara",
            "email": "nusantara@its.ac.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dptsi\\PushNotification\\": "src/"
        },
        "files" : [
            "src/Helpers/All.php"
        ]
    },
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/notifications": "^7.0|^8.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Dptsi\\PushNotification\\Providers\\PushNotificationServiceProvider"
            ]
        }
    }
}
